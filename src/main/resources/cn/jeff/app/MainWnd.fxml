<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="600.0" prefWidth="800.0" style="-fx-font-family: 'Courier New'; -fx-font-size: 20;"
			xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="cn.jeff.app.MainWndJ">
	<top>
		<HBox alignment="CENTER" prefHeight="100.0" spacing="20.0" BorderPane.alignment="CENTER">
			<Button mnemonicParsing="false" onAction="#chooseFile" text="选择文件"/>
			<Button mnemonicParsing="false" onAction="#refresh" text="刷新"/>
		</HBox>
	</top>
	<center>
		<BorderPane BorderPane.alignment="CENTER" fx:id="mainPanel"/>
	</center>
</BorderPane>
